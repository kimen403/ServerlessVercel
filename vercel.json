{
    "version": 2,
    "buildCommand": "npm run vercel-build",
    "builds": [
        {
            "src": "dist/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/index.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    },
    "functions": {
        "dist/index.js": {
            "memory": 1024,
            "maxDuration": 10
        }
    }
}
